                        <script>var rangeslider1 = document.getElementById("myHeat");
                            var x1 = document.getElementById("dCost");
                            var d = document.getElementById("diffCost");

                            var y1 = document.getElementById("eCO2");
                            var y2 = document.getElementById("hCO2");
                            var y3 = document.getElementById("dC02");

                            var a, b, c;
                            rangeslider1.oninput = function () {
                                
                                output1.innerHTML = this.value;
                                x.innerHTML = this.value / 100 * 1.5;
                                x1.innerHTML = this.value / 100 * 2.5;
                                d.innerHTML = x1.innerHTML - x.innerHTML;

                                y2.innerHTML = this.value / 100 * 12146 * 998 - 1 * (1 / (1 - 0.069)) * 1 / 1000 * 1 / 2204.6;
                                y1.innerHTML = this.value / 100 * 40190 * 0.0549 * 1 / 1000;
                                y3.innerHTML = y2.innerHTML - y1.innerHTML;
                            }</script>
                        <span class="w3-opacity">saved</span>
                    </li>
                </ul>
